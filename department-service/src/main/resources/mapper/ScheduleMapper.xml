<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wbm.hospital.mapper.ScheduleMapper">

    <resultMap id="BaseResultMap" type="com.wbm.hospital.entity.Schedule">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="date" column="date" jdbcType="TIMESTAMP"/>
            <result property="departmentCode" column="department_code" jdbcType="VARCHAR"/>
            <result property="doctorId" column="doctor_id" jdbcType="INTEGER"/>
            <result property="doctorName" column="doctor_name" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,date,department_code,
        doctor_id,doctor_name
    </sql>
</mapper>
